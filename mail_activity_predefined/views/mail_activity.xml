<odoo>

<record id="model_mail_activity_form" model="ir.ui.view">
    <field name="model">mail.activity</field>
    <field name="inherit_id" ref="mail.mail_activity_view_form_popup" />
    <field
            name="groups_id"
            eval="[(4, ref('mail_activity_predefined.group_mail_activity_predefined'))]"
        />
    <field name="arch" type="xml">
        <field name="activity_type_id" position="attributes">
            <attribute name="domain">[('predefined', '=', False)]</attribute>
        </field>
    </field>
</record>

</odoo>
